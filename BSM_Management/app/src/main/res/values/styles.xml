<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="BottomNavText" parent="TextAppearance.Material3.LabelSmall">
        <item name="android:textSize">10sp</item>
    </style>
    <style name="Widget.My.BottomNav" parent="Widget.Material3.BottomNavigationView">
        <item name="itemTextAppearanceActive">@style/BottomNavText</item>
        <item name="itemTextAppearanceInactive">@style/BottomNavText</item>
        <item name="itemPaddingBottom">2dp</item>

    </style>
    <style name="Bsm.AssistChip" parent="Widget.Material3.Chip.Assist">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/bsm_text_primary</item>
        <item name="chipCornerRadius">20dp</item>
        <item name="chipStrokeWidth">0dp</item>
        <item name="chipBackgroundColor">@color/bsm_sheet_bg</item>
    </style>


</resources>
